// Test power operator precedence and associativity

fn test() {
    // Basic power
    let a := 2 ** 3;        // Should be 8
    
    // Power has higher precedence than multiplication
    let b := 2 ** 3 * 4;    // Should be (2 ** 3) * 4 = 8 * 4 = 32
    let c := 2 * 3 ** 4;    // Should be 2 * (3 ** 4) = 2 * 81 = 162
    
    // Right-associative: evaluates from right to left
    let d := 2 ** 3 ** 2;   // Should be 2 ** (3 ** 2) = 2 ** 9 = 512
    
    // Unary has higher precedence than power
    let e := -2 ** 3;       // Should be -(2 ** 3) = -8
    
    // Power has higher precedence than addition
    let f := 10 + 2 ** 3;   // Should be 10 + (2 ** 3) = 10 + 8 = 18
    let g := 2 ** 3 + 10;   // Should be (2 ** 3) + 10 = 8 + 10 = 18
    
    // Complex expression
    let h := 2 + 3 * 4 ** 2;  // Should be 2 + (3 * (4 ** 2)) = 2 + (3 * 16) = 2 + 48 = 50
}
