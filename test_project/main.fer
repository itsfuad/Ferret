// add methods on Point
fn (p: utils::Point) normalize() {
    // manual math operation without using std/math by using ** operator
    let length := (p.x ** 2 + p.y ** 2) ** 0.5;
    p.x = p.x / length;
    p.y = p.y / length;
}

