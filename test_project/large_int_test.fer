// Test 128-bit and 256-bit integer support

fn test_large_integers() {
    // i64 max - should use fast path
    let a: i64 = 9223372036854775807;
    
    // i128 - should use BigInt path
    let b: i128 = 170141183460469231731687303715884105727;
    
    // u128 - should use BigInt path
    let c: u128 = 340282366920938463463374607431768211455;
    
    // i256 - should use BigInt path
    let d: i256 = 57896044618658097711785492504343953926634992332820282019728792003956564819967;
    
    // Small values - should use fast path even with large types
    let e: i128 = 42;
    let f: i256 = 1000;
    
    // Hex notation
    let g: u128 = 0xFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFF;
}
