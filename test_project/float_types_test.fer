// Test f128 and f256 float types

fn test_f128() {
    let x: f128 = 3.14159;
    let y: f128 = 2.71828;
    let z: f128 = x + y;
}

fn test_f256() {
    let a: f256 = 1.23456789;
    let b: f256 = 9.87654321;
    let c: f256 = a * b;
}

fn test_conversions() {
    // Lossless conversions to larger types
    let f32_val: f32 = 3.14;
    let f64_from_f32: f64 = f32_val;   // f32 -> f64
    let f128_from_f32: f128 = f32_val; // f32 -> f128
    let f256_from_f32: f256 = f32_val; // f32 -> f256
    
    let f64_val: f64 = 2.718;
    let f128_from_f64: f128 = f64_val; // f64 -> f128
    let f256_from_f64: f256 = f64_val; // f64 -> f256
    
    let f128_val: f128 = 1.414;
    let f256_from_f128: f256 = f128_val; // f128 -> f256
}

fn test_integer_to_float() {
    // Integer to float conversions
    let i32_val: i32 = 42;
    let f128_from_i32: f128 = i32_val; // i32 -> f128 (lossless)
    
    let i64_val: i64 = 12345;
    let f128_from_i64: f128 = i64_val; // i64 -> f128 (lossless)
    
    let i128_val: i128 = 9999999;
    let f256_from_i128: f256 = i128_val; // i128 -> f256 (lossless)
}

fn main() {
    test_f128();
    test_f256();
    test_conversions();
    test_integer_to_float();
}
