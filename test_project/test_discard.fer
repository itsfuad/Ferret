import "std/io";

fn main() {
    let fruits : []str = ["apple", "banana", "cherry"];

    // Test: for let _, _ in fruits
    for let _, _ in fruits {
        io::Println("Fruit ");
    }

    // Test: nested loops with discard
    for let _, _ in fruits {
        for let _, _ in fruits {
            io::Println("Nested");
        }
    }
}

