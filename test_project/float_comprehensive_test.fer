// Comprehensive test for f128 and f256 types

type Vec3 struct {
    .x: f128,
    .y: f128,
    .z: f128
};

fn test_type_inference() {
    // Type inference with untyped literals
    let x: f128 = 3.14;
    let y: f256 = 2.718;
}

fn test_arithmetic() {
    let a: f128 = 10.5;
    let b: f128 = 20.3;
    let sum: f128 = a + b;
    let diff: f128 = a - b;
    let prod: f128 = a * b;
    let quot: f128 = a / b;
}

fn test_comparison() -> bool {
    let x: f128 = 5.5;
    let y: f128 = 10.1;
    return x < y;
}

fn test_function_params(val: f128) -> f256 {
    let result: f256 = val;
    return result;
}

fn test_arrays() {
    // Arrays of f128 and f256
    let arr: [3]f128;
    let dynamic: []f256;
}

fn test_struct_fields() {
    // Struct with f128 fields (declared at module level)
    let v: Vec3;
}

fn main() {
    test_type_inference();
    test_arithmetic();
    let cmp: bool = test_comparison();
    let converted: f256 = test_function_params(123.456);
    test_arrays();
    test_struct_fields();
}
