fn test_large_overflow() {
    // i128 max + 1 should overflow
    let a: i128 = 170141183460469231731687303715884105728;
    
    // u128 max + 1 should overflow
    let b: u128 = 340282366920938463463374607431768211456;
    
    // i256 max + 1 should overflow
    let c: i256 = 57896044618658097711785492504343953926634992332820282019728792003956564819968;
    
    // u256 max + 1 should overflow
    let d: u256 = 115792089237316195423570985008687907853269984665640564039457584007913129639936;
    
    // Negative in unsigned large types
    let e: u128 = -1;
    let f: u256 = -100;

    let min: i8 = 200;
}
