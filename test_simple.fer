import "std/io";
import "std/fs";

fn main() {
    io::Println("Simple Exists test:");
    
    // With variable
    let result := fs::Exists("nonexistent_file_12345.txt");
    if result {
        io::Println("With var: FAIL: true");
    } else {
        io::Println("With var: PASS: false");
    }
    
    // Direct
    if fs::Exists("nonexistent_file_12345.txt") {
        io::Println("Direct: FAIL: true");
    } else {
        io::Println("Direct: PASS: false");
    }
    
    io::Println("Done");
}
