import "std/io";
import "std/fs";

fn main() {
    io::Println("Test 1: Exists on nonexistent");
    let r1 := fs::Exists("xyz123abc.txt");
    if r1 {
        io::Println("r1 = true (BAD)");
    } else {
        io::Println("r1 = false (GOOD)");
    }
    
    io::Println("Test 2: String comparison");
    let a := "hello";
    let b := "hello";
    if a == b {
        io::Println("strings equal (GOOD)");
    } else {
        io::Println("strings not equal (BAD)");
    }
}
