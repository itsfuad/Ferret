import "std/io";

fn main() {
    // Test single values
    io::Println("Hello");
    io::Println(42);
    io::Println(3.14);
    
    // Test multiple values
    io::Println("The answer is", 42);
    io::Println("Pi is approximately", 3.14159);
    io::Println("Multiple types:", 1, 2.5, "three");
    
    // Test Print without newline
    io::Print("Count: ");
    io::Println(100);
    
    // Test various numeric types
    let x: i8 = 127 as i8;
    let y: i64 = 9223372036854775807;
    let z: f32 = 2.5 as f32;
    io::Println("Types:", x, y, z);
}
