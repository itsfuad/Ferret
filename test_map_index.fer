import "std/io";

fn main() {
    let scores := {
        "alpha" => 100,
        "beta" => 200
    };
    
    // Valid access - should work
    io::Println(scores["alpha"]);  // Should print 100
    
    // Invalid access - should panic
    io::Println(scores["gamma"]);  // Should panic: "key not found in map"
}

