import "std/io";

type Status enum {
    Pending,
    Active,
    Completed
};

fn main() {
    let status: Status = Status::Active;

    match status {
        Status::Pending => { io::Println("Pending"); }
        Status::Active => { io::Println("Active"); }
        _ => { io::Println("Other"); }
    }

    let val := 102;

    match val {
        10 => { io::Println("Value is ten"); }
        _ => { io::Println("Value is not ten"); }
    }
}
