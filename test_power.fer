import "std/io";
import "math";

fn main() {
    // Test Sqrt which uses power
    let x := math::Sqrt(16.0);
    io::Println("Sqrt(16) = " + x);
    
    // Direct power tests - power returns f64
    let a := 2.0 ** 3.0;
    io::Println("2.0 ** 3.0 = " + a);
    
    let b := 9.0 ** 0.5;
    io::Println("9.0 ** 0.5 = " + b);
    
    let c := 2 ** 10;
    io::Println("2 ** 10 = " + c);

    io::Println(1.0);
    io::Println(1.23);
}
