import "std/io";

fn main() {
    let a: i32? = 42;

    if a != none {
        // a is narrowed to i32
        io::Println(a);  // should work
        a = 100;  // ok
        io::Println(a);  // should work
    } else {
        // a is narrowed to none
        io::Println("none");  // should work
    }

    // After if, a is back to i32?
    a = none;  // should work
    io::Println("done");
}