import "std/io";

type Counter struct {
    .Value: i32
};

fn (c: &Counter) inc() {
    c.Value++;
}

fn (c: Counter) wontIncr() {
    io::Println(c.Value);
    c.Value++;
    io::Println(c.Value);
}

fn (c: Counter) get() -> i32 {
    return c.Value;
}

fn main() {
    let c := { .Value = 0 } as Counter;
    c.inc();
    c.inc();
    c.wontIncr();
    io::Println(c.get());
}
