import "std/io";

fn divide(a: i32, b: i32) -> str ! i32 {
    if b == 0 {
        return "Division by zero"!;
    }
    return a + b;
}

fn main() {
    let ok := divide(10, 2) catch -1;
    io::Println(ok);

    let fallback := divide(10, 0) catch e {
        io::Println(e);
    } -1;
    io::Println(fallback);

    divide(10, 0) catch e {
        io::Println(e);
        return;
    };

    io::Println(123);
}
