// Test invalid method declarations

// Try to define method on undefined type
fn (p: UndefinedType) method1() -> i32 {
    return 0;
}

// Try to define method on anonymous struct (should fail)
fn (s: struct { .x: i32 }) method2() -> i32 {
    return s.x;
}

// Try to define method on builtin type (should fail based on validation)
fn (n: i32) method3() -> i32 {
    return n + 1;
}

// Valid type
type MyType struct {
    .value: i32
};

// Valid method
fn (m: MyType) getValue() -> i32 {
    return m.value;
}

// Duplicate method (should fail)
fn (m: MyType) getValue() -> i32 {
    return m.value + 1;
}
